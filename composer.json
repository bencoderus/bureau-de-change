{
  "name": "bencoderus/bureau-de-change",
  "description": "A lightweight currency converter with over 40 currencies supports (including cryptocurrencies).",
  "keywords": [
    "php",
    "github",
    "laravel",
    "Currency Converter",
    "laravel 6",
    "laravel 7",
    "laravel 8"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Benjamin Iduwe",
      "email": "bencoderus@gmail.com"
    }
  ],
  "require": {
    "php": "^7.2",
    "guzzlehttp/guzzle": "~6|~7",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": {
      "Bencoderus\\BureauDeChange\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Bencoderus\\BureauDeChange\\Tests\\": "tests"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^8.4|^9.0",
    "mockery/mockery": "^1.4"
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  }
}
